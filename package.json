{
  "name": "ErasTune",
  "version": "1.0.0",
  "description": "Application example built with Angular 15 and hosted on GitHub Pages using GitHub Actions.",
  "author": "(https://github.com/SelmaGuedidi)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "config":"ts-node ./src/assets/setEnv.ts",
    "build": "ng build",
    "build:prod": "npm run config -- --environment=prod && ng build --configuration production --base-href https://SelmaGuedidi.github.io/ErasTune/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless"
  },
  "dependencies": {
    "@angular/animations": "^17.1.1",
    "@angular/cdk": "^17.1.2",
    "@angular/common": "^17.1.1",
    "@angular/compiler": "^17.1.1",
    "@angular/core": "^17.1.1",
    "@angular/fire": "^17.0.1",
    "@angular/forms": "^17.1.1",
    "@angular/platform-browser": "^17.1.1",
    "@angular/platform-browser-dynamic": "^17.1.1",
    "@angular/router": "^17.1.1",
    "@material/slider": "^14.0.0",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.6.7",
    "bootstrap": "^5.3.2",
    "d3": "^7.8.5",
    "font-awesome": "^4.7.0",
    "ngx-toastr": "^18.0.0",
    "openai": "^4.26.0",
    "primeng": "^17.4.0",
    "rxjs": "~7.8.0",
