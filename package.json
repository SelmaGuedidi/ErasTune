    "@types/d3": "^7.4.3",
    "@types/d3-axis": "^3.0.6",
    "@types/d3-format": "^3.0.4",
    "@types/jasmine": "~4.3.1",
    "@types/jquery": "^3.5.29",
    "@types/node": "^20.14.13",
    "@types/webpack-env": "^1.18.4",
    "dotenv": "^16.4.5",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
